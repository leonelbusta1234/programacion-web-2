@{
    ViewData["Title"] = "Lista de Oficinas";
}
<div class="container mt-4">
    <div class="row">
        <div class="col-8">
            <h2 class="text-primary">Lista de Oficinas</h2>
        </div>
        <div class="col-4 text-right">
            <a asp-action="Create" class="btn btn-primary">
                <i class="fa-soild fa-plus"></i>Crear nueva Oficina
            </a>
        </div>
    </div>
    <br />
    <br />
    <table id="tblOficinas" class="table table-striped table-bordered" style="width: 100%">
        <thead>
            <tr>
                <th>Id</th>
                <th>NombreOficina</th>
                <th>Direccion</th>
                <th>Acciones</th>
            </tr>
        </thead>

    </table>
</div>

@section Scripts {
    <script src="~/js/oficina.js"></script>
}
